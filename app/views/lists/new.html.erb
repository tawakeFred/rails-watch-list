<h1>New List</h1>
<div class="row d-flex justify-content-center">
  <div class="col-6">
  <%= simple_form_for @list do |f| %>
    <%= f.input :name %>
    <%= f.input :image, as: :file %>
    <div class="d-grid gap-2">
      <%= f.button :submit, class: "btn btn-primary btn-lg2" %>
    </div>
  <% end %>
  </div>
</div>
